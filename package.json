{
  "name": "rust-yew",
  "description": "Provides some html language features for yew html macro.",
  "author": "TechTheAwesome",
  "publisher": "TechTheAwesome",
  "version": "0.1.0",
  "pricing": "Free",
  "icon": "rustyew.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/TechTheAwesome/code-yew-server/"
  },
  "keywords": [
    "rust",
    "yew",
    "highlighting",
    "language"
  ],
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:rustyew",
    "onLanguage:rust"
  ],
  "main": "./client/out/extension",
  "contributes": {
    "languages": [
      {
        "id": "rust",
        "aliases": [
          "Rust Yew",
          "Rust"
        ],
        "extensions": [
          ".rs"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "injectTo": [
          "source.rust"
        ],
        "scopeName": "source.rust.yew",
        "path": "./syntaxes/yew.injection.json",
        "embeddedLanguages": {
          "source.rust.yew.html": "html",
          "source.rust.yew": "rust"
        },
        "tokenTypes": {
          "source.rust.yew.html": "other",
          "source.rust.yew": "other"
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w",
    "lint": "eslint ./client/src ./server/src --ext .ts,.tsx",
    "postinstall": "cd client && npm install && cd ../server && npm install && cd ..",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.2.6",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.13.0",
    "jest": "^29.3.1",
    "mocha": "^9.2.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
